{
  "name": "marketeermerchantapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pod": "cd ios && pod install",
    "clean": "cd android && gradlew clean",
    "android": "cd android && gradlew clean && cd .. && react-native run-android",
    "ios": "react-native run-ios",
    "iosdevice": "react-native run-ios --device",
    "iphone8": "react-native run-ios --simulator='iPhone 8 Plus'",
    "iphonese": "react-native run-ios --simulator='iPhone SE (2nd generation)'",
    "ipadpro4": "react-native run-ios --simulator='iPad Pro (12.9-inch) (4th generation)'",
    "ipadpro2": "react-native run-ios --simulator='iPad Pro (12.9-inch) (2nd generation)'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "patch-package && npx jetify",
    "createdebugapk": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && gradlew assembleDebug",
    "release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && gradlew clean && gradlew assembleRelease"
  },
